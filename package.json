{
  "name": "genius-swhf51-3000",
  "description": "A NodeJS / Express app that creates a web UI and API for Genius SW-HF5.1 3000 - speaker system",
  "version": "0.0.1",
  "private": true,
  "author": "Dick Swart <dick@swart.ninja>",
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "cross-env NODE_ENV=development DEBUG=genius-swhf51-3000:* node ./bin/www",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DickSwart/genius-swhf51-3000"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "lirc_node": "0.0.4",
    "lodash": "^4.17.19",
    "morgan": "~1.9.1",
    "pug": "^2.0.4",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
